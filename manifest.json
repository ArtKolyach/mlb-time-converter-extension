{
  "name": "MLB Time Converter",
  "description": "Изменяет время матчей с сайта MLB на Мск",
  "version": "0.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://www.mlb.com/",
        "https://www.mlb.com/schedule/*"
      ],
      "js": [
        "main-script.js"
      ]
    }
  ]
}
